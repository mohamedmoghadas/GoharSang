
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_layout.cshtml";
}
@model GoharSang.Models.vmModel.vmReportBargirt
<div class="container dailyCol">
    <div class="row dailyRow">
        <div class="col dailyLDiv">
            <a class="btn btn-primary d-block dailyBtnTop" href="~/Home/Index" role="button">صفحه اصلی</a>
            <br>
            <label class="d-block dailyTitle">
                فیلتر های | نوع کوپ | تعداد
            </label>
            <div class="table-responsive-sm">
                <table class="table table-info table-striped dailyTable">

                    <thead>
                        <tr>
                            <td>ردیف</td>
                            <td>نوع کوپ</td>
                            <td>تعداد</td>
                            <td>انبار</td>
                            <td>معدن</td>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            int i = 0;
                        }
                        @foreach (var item in Model.list)
                        {
                            i++;
                            <tr>
                                <td>@i</td>
                                <td>@item.copname</td>
                                <td>@item.RecordEntryExitOrderCount</td>
                                <td>@item.StoreName</td>
                                <td>@item.minename</td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
            <button class="btn btn-primary dailyBtnBottom" onclick="window.print()">چاپ گزارش</button>
            <button class="btn btn-primary dailyBtnBottom2" onclick="location.href='@Url.Action("export", "ReportdenotativeStore")'">خروجی اکسل</button>
        </div>
    </div>
</div>
